{
    "name": "hbase-read",
    "type": "spark",
    "mounts": [
      {
        "resourceName": "git-demo"
      }
    ],
    "spark": {
      "file": "hbase/hbase-read.py",
      "jars": [
        "hbase/runtime/7.1.8/hbase-shaded-client-2.2.3.7.1.7.2000-305.jar",
        "hbase/runtime/7.1.8/hbase-shaded-mapreduce-2.2.3.7.1.7.2000-305.jar",
        "hbase/runtime/7.1.8/hbase-spark3-1.0.0.3.3.7180.0-274.jar",
        "hbase/runtime/7.1.8/hbase-spark3-protocol-shaded-1.0.0.3.3.7180.0-274.jar",
        "hbase/runtime/7.1.8/opentelemetry-api-0.12.0.jar",
        "hbase/runtime/7.1.8/opentelemetry-context-0.12.0.jar",
        "hbase/runtime/7.1.8/scala-library-2.12.15.jar",
        "hbase/runtime/7.1.8/scala-parser-combinators_2.12-1.1.2.jar"
      ],
      "files": [
        "hbase/datahub-base-cluster/xhu-718/hbase-site.xml"
      ],
      "driverMemory": "1g",
      "driverCores": 1,
      "executorMemory": "1g",
      "executorCores": 1,
      "conf": {
        "dex.safariEnabled": "false",
        "spark.driver.extraClassPath": "/app/mount/hbase/datahub-base-cluster/xhu-718",
        "spark.pyspark.python": "python3"
      },
      "logLevel": "INFO"
    },
    "retentionPolicy": "keep_indefinitely",
    "schedule": {
      "enabled": false,
      "user": "cdp_xhu"
    }
  }